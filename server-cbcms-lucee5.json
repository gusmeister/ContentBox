{
    "app": {
        "cfengine": "lucee@5",
        "serverHomeDirectory": ".engines/lucee5"
    },
    "name": "cbcms-lucee5",
    "JVM": {
        "heapSize": "768"
    },
    "web": {
		"directoryBrowsing" : true,
        "rewrites": {
            "enable": "true"
        },
        "http": {
            "port": "8589"
        }
    },
    "trayicon": "modules_app/contentbox-custom/_content/ContentBox_300.png",
    "trayOptions": [{
        "label": "Open ContentBox Admin",
        "action": "openbrowser",
        "url": "http://localhost:\\${runwar.port}/cbadmin",
        "disabled": false,
        "image": "modules_app/contentbox-custom/_content/ContentBox-Circle_32.png"
    }]
}